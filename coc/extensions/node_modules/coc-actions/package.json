{
  "name": "coc-actions",
  "description": "Actions menu for coc.nvim",
  "version": "1.4.0",
  "publisher": "iamcco",
  "engines": {
    "coc": "^0.0.74"
  },
  "keywords": [
    "coc.nvim",
    "actions",
    "quickFix"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/index.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-actions configuration",
      "properties": {
        "coc-actions.hideCursor": {
          "type": "boolean",
          "default": true,
          "description": "Hide cursor when open actions menu"
        },
        "coc-actions.showActionKind": {
          "type": "boolean",
          "default": true,
          "description": "Show action kind"
        },
        "coc-actions.useCursorLine": {
          "type": "boolean",
          "default": false,
          "description": "Using cursorline for active line, this avoids tail whitespace"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamcco/coc-actions"
  },
  "bugs": {
    "url": "https://github.com/iamcco/coc-actions"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./out",
    "build": "webpack",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@types/node": "^13.1.2",
    "@types/semver": "^6.2.0",
    "coc.nvim": "^0.0.74",
    "semver": "^7.1.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "vscode-languageserver-protocol": "^3.14.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
